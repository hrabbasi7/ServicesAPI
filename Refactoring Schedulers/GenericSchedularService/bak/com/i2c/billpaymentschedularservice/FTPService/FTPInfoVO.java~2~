public class FTPInfoVO {
  private String ftpAddress;
  private String ftpPort;
  private String ftpUserId;
  private String ftpPassword;
  private String ftpLocalPath;
  private String ftpRemotePath;
  private String isPgp;
  private String isFtpAllowed;
  private String filePreFix;
  private String filePostFix;
  private String isNullFile;
  private String schemeId;
  private String isZip;
  private String pgpPassPhrase;
  private String paymentFilePrefix;
  public FTPInfoVO() {
  }

  public void setFtpAddress(String ftpAddress) {
    this.ftpAddress = ftpAddress;
  }

  public void setFtpPort(String ftpPort) {
    this.ftpPort = ftpPort;
  }

  public void setFtpUserId(String ftpUserId) {
    this.ftpUserId = ftpUserId;
  }

  public void setFtpPassword(String ftpPassword) {
    this.ftpPassword = ftpPassword;
  }

  public void setFtpLocalPath(String ftpLocalPath) {
    this.ftpLocalPath = ftpLocalPath;
  }

  public void setFtpRemotePath(String ftpRemotePath) {
    this.ftpRemotePath = ftpRemotePath;
  }

  public void setIsPgp(String isPgp) {
    this.isPgp = isPgp;
  }

  public void setIsFtpAllowed(String isFtpAllowed) {

    this.isFtpAllowed = isFtpAllowed;
  }

  public void setFilePreFix(String filePreFix) {

    this.filePreFix = filePreFix;
  }

  public void setFilePostFix(String filePostFix) {

    this.filePostFix = filePostFix;
  }

  public void setIsNullFile(String isNullFile) {
    this.isNullFile = isNullFile;
  }

  public void setSchemeId(String schemeId) {
    this.schemeId = schemeId;
  }

  public void setIsZip(String isZip) {
    this.isZip = isZip;
  }

  public void setPgpPassPhrase(String pgpPassPhrase) {

    this.pgpPassPhrase = pgpPassPhrase;
  }

  public void setPaymentFilePrefix(String paymentFilePrefix) {
    this.paymentFilePrefix = paymentFilePrefix;
  }

  public String getFtpAddress() {
    return ftpAddress;
  }

  public String getFtpPort() {
    return ftpPort;
  }

  public String getFtpUserId() {
    return ftpUserId;
  }

  public String getFtpPassword() {
    return ftpPassword;
  }

  public String getFtpLocalPath() {
    return ftpLocalPath;
  }

  public String getFtpRemotePath() {
    return ftpRemotePath;
  }

  public String getIsPgp() {
    return isPgp;
  }

  public String getIsFtpAllowed() {

    return isFtpAllowed;
  }

  public String getFilePreFix() {

    return filePreFix;
  }

  public String getFilePostFix() {

    return filePostFix;
  }

  public String getIsNullFile() {
    return isNullFile;
  }

  public String getSchemeId() {
    return schemeId;
  }

  public String getIsZip() {
    return isZip;
  }

  public String getPgpPassPhrase() {

    return pgpPassPhrase;
  }

  public String getPaymentFilePrefix() {
    return paymentFilePrefix;
  }
}
