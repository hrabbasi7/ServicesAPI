package com.i2c.billpaymentschedularservice.excep;
import com.i2c.billpaymentschedularservice.excep.*;

/**
 * Title:        MCP
 * Description:
 * Copyright:    Copyright (c) 2002
 * Company:      i2c Inc.
 * @author barshad
 * @version 1.0
 */

public class DatabaseExcep extends BaseExcep {

	public DatabaseExcep(int errorcode) {
		super(errorcode);
	}//end store

	public DatabaseExcep(int errorcode, String message) {
		super(errorcode,message);
	}//end store value method

	public DatabaseExcep(Throwable excep, String message) {
		super(excep,message);
	}//end store value method

	public DatabaseExcep(int errorcode, Throwable excep) {
		super(errorcode,excep);
	}//end store value method

	public DatabaseExcep(Throwable excep) {
		super(excep);
	}//end store value method

	public DatabaseExcep(int errorcode,Throwable excep,String message) {
		super(errorcode,excep,message);
	}//end store value method


	public DatabaseExcep(Throwable excep,String message,String method) {
		super( excep,message,method );
	}//end store value method

	public DatabaseExcep(int errorcode,Throwable excep,String message,String method) {
		super(errorcode,excep,message,method );
	}//end store value method


	public DatabaseExcep(Throwable excep,String message,String method,String classname) {
		super(excep,message,method,classname );
	}//end store value method

	public DatabaseExcep(int errorcode,Throwable excep,String message,String method,String classname) {
		super(errorcode,excep,message,method,classname );
	}//end store value method



}//end class
